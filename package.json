{
  "name": "carsharing-monitor-telegram-bot",
  "version": "0.0.1",
  "description": "Телеграм бот для мониторинга свободных каршеринговых автомобилей",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "start": "npm run build:live",
